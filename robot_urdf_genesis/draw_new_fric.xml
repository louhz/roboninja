<?xml version='1.0' encoding='utf-8'?>
<mujoco model="graspable_objects">
  <compiler meshdir="" angle="radian" coordinate="local" strippath="false" discardvisual="false" inertiafromgeom="true" />

  <option gravity="0 0 -9.81" integrator="RK4" timestep="0.001" cone="pyramidal" impratio="5" tolerance="1e-8">
    <flag warmstart="enable" />
  </option>

  <asset>
    <mesh name="draw1" file="meshes/Draw/draw1.stl" />
    <mesh name="draw2" file="meshes/Draw/draw2.stl" />
    <material name="rubber" rgba="0.0 0.4 0.5 1" />
    <material name="mat_floor" rgba="0.9 0.9 0.9 1" />
  </asset>

  
  <default>
    
    <geom friction="8 0.01 0.001" solref="0.005 1" solimp="0.95 0.99 0.001" condim="6" contype="1" conaffinity="1" />

    
    <default class="col">
      <geom friction="8 0.01 0.001" margin="0.002" solref="0.003 1" solimp="0.95 0.99 0.001" condim="6" contype="1" conaffinity="1" />
    </default>

    
    <default class="vis">
      <geom contype="0" conaffinity="0" group="1" density="0" friction="8 0.01 0.001" />
    </default>
  </default>

  <worldbody>

    
    <geom name="floor" type="plane" size="2 2 0.05" material="mat_floor" friction="8 0.01 0.001" />

    
    <body name="draw1" pos="0.613 -0.478 0.196" quat="0.987964 0.003288 0.003636 0.154607">
      <joint name="draw1_free" type="free" damping="0.5" armature="0.001" />

      
      <geom name="draw1_vis" class="vis" type="mesh" mesh="draw1" material="rubber" friction="8 0.01 0.001" />

      
      <geom name="draw1_col_box" class="col" type="box" size="0.035 0.025 0.010" density="1500" friction="8 0.01 0.001" />

      
      

      
      <site name="draw1_grasp" pos="0 0 0" size="0.005" rgba="0.8 0.2 0.2 1" />
    </body>

    
    <body name="draw2" pos="0.25 -0.521 0.196" quat="0.365226 0.351452 0.607535 0.611549">
      
      <geom name="draw2_vis" class="vis" type="mesh" mesh="draw2" rgba="0.1 0.1 0.1 1" friction="8 0.01 0.001" />
    </body>

  </worldbody>
</mujoco>